{
  "coc.preferences.enableMessageDialog": true,
  "notification.statusLineProgress": false,
  "notification.border": false,
  "notification.marginRight": 0,
  "notification.winblend": 0,
  "suggest.floatConfig": {
    "borderhighlight": "FloatBorder",
    "highlight": "CocSuggestFloating"
  },
  "suggest.labelMaxLength": 60,
  "suggest.formatItems": [
    "kind",
    "abbr",
    "menu",
    "shortcut"
  ],
  "suggest.completionItemKindLabels": { //{{{
    "keyword": "",
    "variable": "",
    "value": "",
    "operator": "",
    "constructor": "",
    "function": "",
    "reference": "",
    "constant": "",
    "method": "",
    "struct": "",
    "class": "",
    "interface": "",
    "text": "",
    "enum": "",
    "enumMember": "",
    "module": "",
    "color": "",
    "property": "",
    "field": "",
    "unit": "",
    "event": "",
    "file": "",
    "folder": "",
    "snippet": "",
    "typeParameter": "",
    "default": ""
  }, //}}}
  "hover.previewMaxHeight": 12,
  "hover.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder"
  },
  "diagnostic.enableSign": false,
  // "diagnostic.infoSign": "",
  // "diagnostic.errorSign": "",
  // "diagnostic.warningSign": "",
  // "diagnostic.hintSign": "",
  "diagnostic.enableMessage": "jump",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder"
  },
  "diagnostic.refreshOnInsertMode": true,
  "signature.floatConfig": {
    "border": true,
    "borderhighlight": "FloatBorder"
  },
  "dialog.rounded": false,
  "stylua.styluaPath": "/usr/local/bin/stylua",
  "languageserver": {
