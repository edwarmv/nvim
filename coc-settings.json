    "sumneko_lua": { //{{{
      "command": "lua-language-server",
      "filetypes": [
        "lua"
      ],
      "rootPatterns": [
        ".luarc.json",
        ".luacheckrc",
        ".stylua.toml",
        "selene.toml"
      ],
      "settings": {
        "Lua": {
          "telemetry": {
            "enable": false
          }
        }
      },
      "formatterPriority": 0
    }, //}}}
