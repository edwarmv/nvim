{
  "html.autoClosingTags": true,
  "html.format.wrapLineLength": 80,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "jst"
  ],
  "emmet.includeLanguages": {"jst": "html"},
  "css.trace.server": "off",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.preferences.quoteStyle": "single",
  "coc.preferences.formatOnSaveFiletypes": [
  ],
  "signature.target": "float",
  "suggest.keepCompleteopt": true,
  "suggest.echodocSupport": false,
  "suggest.floatEnable": true,
  "suggest.detailField": "abbr",
  "suggest.enablePreview": true,
  "suggest.enablePreselect": false,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic.messageTarget": "float",
  "diagnostic.enableMessage": "always",
  "diagnostic.virtualText": false,
  "diagnostic.checkCurrentLine": false,
  "diagnostic.errorSign": "▍",
  "diagnostic.warningSign": "▍",
  "diagnostic.infoSign": "▍",
  "diagnostic.hintSign": "▍",
  "coc.preferences.floatActions": true,
  "list.statusLineSegments": [
    " (%L/%{get(b:list_status, \"total\", \"\")})",
    "%{get(b:list_status, \"loading\", \"\")}",
    "%{get(b:list_status, \"args\", \"\")}",
    "%=",
    "%#CocListPath# %{get(b:list_status, \"cwd\", \"\")} %l/%L%* "
  ],
  "list.source.extensions.defaultOptions": ["--number-select"],
  "highlight.disableLanguages": [".csv", "csv"],
  "flutter.sdk.flutter-lookup": "asdf which flutter",
  "flutter.sdk.dart-lookup": "asdf which dart",
  "flutter.lsp.initialization.closingLabels": false,
  "flutter.trace.server": "off",
  "angular.experimental-ivy": false,
  "angular.enable-experimental-ivy-prompt": false,
  "angular.trace.server": "verbose",
  "explorer.icon.enableNerdfont": true,
  "languageserver": {
    "tsserver": {
      "command": "typescript-language-server",
       "args": [
        "--stdio"
      ],
      "filetypes": [
        "typescript"
      ]
    }
  }
}

